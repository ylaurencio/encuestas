function FindProxyForURL(url, host)
{
        if (
            shExpMatch(host, "10.*" )
        ||  shExpMatch(host, "172.*" )
        ||  shExpMatch(host, "192.168.*" )
        ||  shExpMatch(host, "127.*" )
        ||  shExpMatch(host, "localhost")
        ||  shExpMatch(host, "*.serviciosdigitales.gob.cu")
        ||  shExpMatch(host, "*.jovenclub.cu")
        ||  shExpMatch(host, "*.tinored.cu")
        ||  shExpMatch(host, "*.ludox.cu")
        ||  shExpMatch(host, "*.ecured.cu")
        ||  shExpMatch(host, "*.cubava.cu")
        ||  shExpMatch(host, "cubava.cu")
        ||  shExpMatch(host, "*.fdc.com")
        ||  isPlainHostName(host)
        ) {
            return "DIRECT";
          }
	if (
            isInNet(myIpAddress(), "10.17.0.0", "255.255.0.0")
	) {
	    return "PROXY proxy.pal.jovenclub.cu:3128";
	  }

	if (
            isInNet(myIpAddress(), "10.18.0.0", "255.255.0.0")
	) {
	    return "PROXY proxy.dn.jovenclub.cu:3128";
	  }
	if (
            isInNet(myIpAddress(), "192.168.1.0", "255.255.255.0")
            || isInNet(myIpAddress(), "10.0.0.0", "255.224.0.0")
	) {
            return "HTTPS proxy-pap.jovenclub.cu:3129";
          }
}
